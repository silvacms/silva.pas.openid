<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal">
  <body>
    <h1 tal:replace="structure here/manage_page_header">Header</h1>

    <h2 tal:define="manage_tabs_message options/manage_tabs_message | nothing"
        tal:replace="structure here/manage_tabs">Tabs</h2>

    <p> Details on OpenID Member </p>

    <table cellspacing="0" cellpadding="2" border="0">
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Id
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/id">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Identity URL
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/identity_url">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Fullname
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/fullname">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Email
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/email">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Fully registered ?
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/fully_registered">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Application date
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/application_date">
        </td>
      </tr>
      <tr>
        <td align="left" valign="top">
          <div class="form-label">
            Last login date
          </div>
        </td>
        <td align="left" valign="top"
            tal:content="here/last_login_date">
        </td>
      </tr>
    </table>
  </body>
</html>

